{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "facethefacts/api:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8000",
      "HostPort": "80"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current",
      "ContainerDirectory": "/var/app/current"
    }
  ],
  "Logging": "/var/log/api"
}
